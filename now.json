{
  "version": 2,
  "alias": "talk-tlp-2019.axelhzf.com",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "assets/$1" },
    { "src": "/(.*)\\.js", "dest": "$1.js" },
    { "src": "/(.*)", "dest": "index.html" }
  ]
}
