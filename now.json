{
  "version": 2,
  "alias": "talk-tlp-2019.axelhzf.com",
  "builds": [
    {
      "src": "dist/**",
      "use": "@now/static"
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "dist/assets/$1" },
    { "src": "/(.*)\\.js", "dest": "dist/$1.js" },
    { "src": "/(.*)", "dest": "dist/index.html" }
  ]
}
